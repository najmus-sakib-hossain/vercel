{
  "name": "ui",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "manfromexistence",
    "url": "https://twitter.com/manfromexistence"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "templates/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:cli": "turbo --filter=manfromexistence build",
    "build:registry": "pnpm --filter=www build:registry && pnpm --filter=www lint:fix && pnpm format:write -- --loglevel silent",
    "registry:build": "pnpm --filter=www build:registry && pnpm --filter=www lint:fix && pnpm format:write -- --loglevel silent",
    "registry:capture": "pnpm --filter=www registry:capture",
    "dev": "turbo run dev --parallel",
    "cli:dev": "turbo --filter=manfromexistence-ui dev",
    "cli:start": "pnpm --filter=manfromexistence-ui start:dev",
    "cli:build": "pnpm --filter=manfromexistence-ui build",
    "manfromexistence:dev": "turbo --filter=manfromexistence dev",
    "manfromexistence": "pnpm --filter=manfromexistence start:dev",
    "manfromexistence:prod": "pnpm --filter=manfromexistence start:prod",
    "manfromexistence:build": "pnpm --filter=manfromexistence build",
    "manfromexistence:test": "pnpm --filter=manfromexistence test",
    "docs:build": "pnpm --filter=www build:docs",
    "www:dev": "pnpm --filter=www dev",
    "www:build": "pnpm --filter=www build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "preview": "turbo run preview",
    "typecheck": "turbo run typecheck",
    "format:write": "turbo run format:write",
    "format:check": "turbo run format:check",
    "sync:templates": "./scripts/sync-templates.sh \"templates/*\"",
    "check": "turbo lint typecheck format:check",
    "release": "changeset version",
    "pub:beta": "cd packages/manfromexistence && pnpm pub:beta",
    "pub:release": "cd packages/manfromexistence && pnpm pub:release",
    "test": "turbo run test --force"
  },
  "packageManager": "pnpm@9.0.6",
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@typescript-eslint/parser": "^5.59.7",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.9.9",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "3.13.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "puppeteer": "^23.6.0",
    "tailwindcss": "3.4.6",
    "tailwindcss-animate": "^1.0.5",
    "tsx": "^4.1.4",
    "turbo": "^1.9.9",
    "vite": "^5.4.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^2.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "typescript": "^5.5.3"
  }
}
